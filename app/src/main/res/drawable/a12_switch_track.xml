<?xml version="1.0" encoding="utf-8"?>
<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">

	<item>

		<shape android:shape="rectangle">

			<corners android:radius="12dp" />

			<size
				android:width="48dp"
				android:height="24dp"
				/>

			<solid android:color="#666" />

		</shape>

	</item>

	<item
		android:drawable="@drawable/ic_outline_check_24"
		android:left="4dp"
		android:right="24dp"
		android:top="2dp"
		android:bottom="2dp"
		android:width="20dp"
		android:height="20dp"
		/>

	<item
		android:drawable="@drawable/ic_outline_remove_24"
		android:left="24dp"
		android:right="4dp"
		android:top="2dp"
		android:bottom="2dp"
		android:width="20dp"
		android:height="20dp"
		/>

</layer-list>
